<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
    <!-- Greeting -->
    <category>
        <pattern>Hi *</pattern>
        <template>
            <random>
                <li>Hello there!</li>
                <li>Hey, how can I assist you?</li>
                <li>What's up?</li>
            </random>
        </template>
    </category>
    <category>
        <pattern>Hello *</pattern>
        <template>
            <srai>Hi *</srai>
        </template>
    </category>
    <category>
        <pattern>Hey *</pattern>
        <template>
            <srai>Hi *</srai>
        </template>
    </category>
    <category>
        <pattern>TURN ON LIGHT</pattern>
        <template>Light is turned on</template>
    </category>
    <category>
        <pattern>PLEASE TURN ON LIGHT</pattern>
        <template>Light is turned on</template>
    </category>
    <category>
        <pattern>TURN ON LIGHT PLEASE</pattern>
        <template>Light is turned on</template>
    </category>
    <category>
        <pattern>WOULD YOU PLEASE TURN ON LIGHT</pattern>
        <template>Light is turned on</template>
    </category>
    <category>
        <pattern>* TURN ON LIGHT *</pattern>
        <template>Light is turned on</template>
    </category>

    <category>
        <pattern>TURN OFF LIGHT</pattern>
        <template>Light is turned off</template>
    </category>
    <category>
        <pattern>PLEASE TURN OFF LIGHT</pattern>
        <template>Light is turned off</template>
    </category>
    <category>
        <pattern>TURN OFF LIGHT PLEASE</pattern>
        <template>Light is turned off</template>
    </category>
    <category>
        <pattern>WOULD YOU PLEASE TURN OFF LIGHT</pattern>
        <template>Light is turned off</template>
    </category>
    <category>
        <pattern>* TURN OFF LIGHT *</pattern>
        <template>Light is turned off</template>
    </category>

    <category>
        <pattern>TURN ON FAN</pattern>
        <template>Fan is turned on</template>
